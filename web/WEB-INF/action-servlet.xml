<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean name="/initOciIndex,/initOciIndexFrame" class="com.jcatalog.ocisetest.webapp.actions.InitOciIndexAction">
    <property name="propertiesHolders"><ref bean="propertiesHolderFactory"/></property>
  </bean>

  <bean name="/initPunchoutIndex" class="com.jcatalog.ocisetest.webapp.actions.InitPunchoutIndexAction">
    <property name="schemaHolderFactory"><ref bean="schemaHolderFactory"/></property>
  </bean>

  <bean name="/showOciParameters" class="com.jcatalog.ocisetest.webapp.actions.ShowOciParametersAction">
    <property name="propertiesHolders"><ref bean="propertiesHolderFactory"/></property>
  </bean>

  <bean name="/saveOciConfiguration" class="com.jcatalog.ocisetest.webapp.actions.SaveOciConfigurationAction">
    <property name="propertiesHolders"><ref bean="propertiesHolderFactory"/></property>
  </bean>

  <bean name="/deletePunchoutSchema,/editPunchoutSchema,/addPunchoutProperty,/savePunchoutSchema,/duplicatePunchoutSchema,
    /deletePunchoutProperty,/setActive" class="com.jcatalog.ocisetest.webapp.actions.PunchoutSchemaManagerAction">
    <property name="schemaHolderFactory"><ref bean="schemaHolderFactory"/></property>
  </bean>

  <bean name="/copyItem,/deleteItem,/saveItem,/showInboundData" class="com.jcatalog.ocisetest.webapp.actions.PunchoutTestAction">
    <property name="schemaHolderFactory"><ref bean="schemaHolderFactory"/></property>
    <property name="itemStorageManager"><ref bean="itemStorageManager"/></property>
  </bean>

  <bean name="/processOciRequest" class="com.jcatalog.ocisetest.webapp.actions.ProcessOciRequestAction">
    <property name="ociParametersEncrypter"><ref bean="ibmOciRequestEncrypter"/></property>
  </bean>

</beans>