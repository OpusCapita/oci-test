<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean name="/init" class="com.jcatalog.ocisetest.webapp.actions.InitAction">
    <property name="propertiesHolders"><ref bean="propertiesHolderFactory"/></property>
  </bean>

  <bean name="/showProperties" class="com.jcatalog.ocisetest.webapp.actions.ShowAction">
    <property name="propertiesHolders"><ref bean="propertiesHolderFactory"/></property>
  </bean>

  <bean name="/saveConfiguration" class="com.jcatalog.ocisetest.webapp.actions.SaveAction">
    <property name="propertiesHolders"><ref bean="propertiesHolderFactory"/></property>
  </bean>

</beans>